      },
      {
        "type": "checkbox",
        "id": "enable_rates",
        "label": "Enable shipping rates calculator",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "enable_discount",
        "label": "Enable input box discounts codes",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "enable_gift_wrap",
