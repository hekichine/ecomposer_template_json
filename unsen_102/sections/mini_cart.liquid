      },
      {      
        "type": "paragraph", 
        "content": "------------------------------"
      },
      {      
        "type": "paragraph", 
        "content": "Those settings are only applicable when the cart contains at least one product."
      },
      {
        "type": "select",
        "id": "style",
        "options": [
          {"value": "1","label": "Icon"},
          {"value": "2","label": "Button text"}
        ],
        "label": "Buttons style:"
      },
      {
        "type": "select",
        "id": "pos",
        "default": "2",
        "options": [
          {"value": "1","label": "Top"},
          {"value": "2","label": "Bottom"}
        ],
