{%- comment -%}
  Comments may not appear right after they are submitted, either to be checked by Shopify's spam filter
  or to await moderation. When a comment is submitted, the browser is redirected to a page
  that includes the new comment id in its URL.
    Example: http://shopname.myshopify.com/blogs/news/2022072-my-post?comment=3721372
{%- endcomment -%}
<!-- sections/article-stickiy-title.liquid -->
{{ 'section.css' | asset_url | stylesheet_tag }}
{{ 'main-article.css' | asset_url | stylesheet_tag }}
{{ 'button-style.css' | asset_url | stylesheet_tag }}
<link href="{{ 'custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
    assign sid = section.id
    assign se_stts = section.settings  
    assign stt_layout = se_stts.layout
    assign stt_image_bg = se_stts.image_bg
    if stt_layout == 't4s-se-container' 
        assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
    elsif stt_layout == 't4s-container-wrap'
        assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
    else
        assign html_layout = '__' | split: '__'
    endif 

    assign article_id = article.id
    assign blog_url = blog.url
    assign image = article.image 
    assign article_title = article.title
    assign article_tags = article.tags
    assign article_metafields = article.metafields.theme 

    if comment.status == 'pending'
        assign number_of_comments = article.comments_count | plus: 1 
    else 
        assign number_of_comments = article.comments_count 
    endif  
 -%}
<div class="t4s-section-inner t4s_nt_se_{{ sid }} {{ stt_layout }}{% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} t4s-has-imgbg lazyloadt4s{% endif %}" {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{% endif %} {% render 'section_style', se_stts: se_stts -%} >
    {{- html_layout[0] -}}
    {%- if stt_layout == 't4s-se-container' -%}
    <div class="t4s-container-inner{% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s{% endif %}" {% if stt_image_bg != blank %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{% endif %}>{% endif -%}
        <div class="t4s-row">
            {%- if se_stts.feature_image -%}
                <div class="t4s-col-item t4s-col-12 t4s-feature-image">
                    <div class="t4s-article-image t4s-pr" style="max-width:{{ image.width }}px;margin-left: auto;margin-right: auto;">                 
                        <img class="lazyloadt4s" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}">
                        <span class="lazyloadt4s-loader is-bg-img" style="background: url({{ image | image_url: width: 1 }});"></span>    
                        {%- if article_metafields.video_url != blank or article_metafields.gallery != blank -%}
                            <div class="t4s-article-metafields">
                                {%- if article_metafields.video_url != blank -%}
                                    <div data-open-mfp-iframe class="t4s-article-metafields__video" href="{{ article_metafields.video_url }}" data-tooltip="right" title="{{ 'blogs.article.click_video' | t }}">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 512 512"><path d="M443.9,196.9L141.5,10.5C119.6-3,93.1-3.5,70.7,9c-22.4,12.5-35.8,35.3-35.8,61v371.1c0,38.8,31.3,70.6,69.8,70.8c0.1,0,0.2,0,0.3,0c12,0,24.6-3.8,36.3-10.9c9.4-5.7,12.4-18,6.6-27.4c-5.7-9.4-18-12.4-27.4-6.6c-5.5,3.3-10.8,5.1-15.6,5.1 c-14.5-0.1-30.1-12.5-30.1-30.9V70.1c0-11,5.7-20.8,15.4-26.2c9.6-5.4,21-5.1,30.3,0.6l302.4,186.4c9.1,5.6,14.3,14.9,14.3,25.6 c0,10.7-5.3,20-14.4,25.6L204.1,415.9c-9.4,5.8-12.4,18-6.6,27.5c5.8,9.4,18,12.4,27.5,6.6l218.6-133.9 c20.9-12.8,33.5-35,33.5-59.5C477.2,232.1,464.7,209.8,443.9,196.9z"/></svg>
                                    </div>
                                {%- endif -%}
                                {%- if article_metafields.gallery != blank -%}
                                    {%- capture gallery_json %}[{% for image in article_metafields.gallery.value -%}
                                    { "src": {{ image.src | image_url | json }}, "w": {{ image.width }}, "h": {{ image.height }}, "alt": {{ image.alt | json }} }{% unless forloop.last %},{% endunless -%}
                                    {%- endfor %}]{% endcapture -%}
                                    <div class="t4s-article-metafields__images" data-pswp-images-trigger='{{ gallery_json }}' data-tooltip="right" title="{{ 'blogs.article.click_gallery' | t }}">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 512 512">
                                            <polygon points="480,480 224,480 224,512 512,512 512,224 480,224"/>
                                            <path d="M448,0H0v448h448V0z M416,416H32v-57.4L134.6,256h50.8l64,64h48.5l64-128H416V416z M416,160h-73.9l-64,128h-15.5l-64-64 h-77.2L32,313.4V32h384V160z"/>
                                            <rect x="480" y="160" width="32" height="32"/>
                                            <rect x="160" y="480" width="32" height="32"/>
                                            <path d="M128,192c35.3,0,64-28.7,64-64s-28.7-64-64-64s-64,28.7-64,64S92.7,192,128,192z M128,96c17.7,0,32,14.3,32,32 s-14.3,32-32,32s-32-14.3-32-32S110.3,96,128,96z"/>
                                        </svg>
                                    </div>
                                {%- endif -%}
                            </div>
                        {%- endif -%}            
                    </div>
                </div>
            {%- endif -%}
            <div class="t4s-col-item t4s-col-lg-3 t4s-col-md-3 t4s-col-12 t4s-sidebar-article-sticky">
                <div class="t4s-sidebar-sticky t4s-text-start">
                    {%- if se_stts.show_blog_title != 'disable' -%}
                        <div class="t4s-article-category">
                            {%- if se_stts.show_blog_title == 'blog_current' -%}
                                <a href="{{ blog_url }}">{{ blog.title }}</a>
                            {%- else -%}
                                {%- assign article_tags = article.tags | join: ';' | split: ';' -%}
                                {%- assign blog_tags = article_tags | where: 'blog_' -%}
                                {%- for tag in blog_tags -%}
                                    <a href="{{ blog_url }}/tagged/{{ tag | handle }}">{{ tag | remove : 'blog_' | strip }}</a> {%- unless forloop.last -%} ,  {% endunless -%}
                                {%- endfor -%}
                            {%- endif -%}
                        </div>
                    {%- endif -%}
                    <h3 class="t4s-article-title"><a href="{{ article.url }}">{{ article_title }}</a></h3>
                    {%- if se_stts.show_tags -%}
                        {%- assign article_tags = article.tags | join: ';' | split: ';' -%}
                        <div class="t4s-article-tags t4s-text-start">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-tag"><path d="M20.59 13.41l-7.17 7.17a2 2 0 0 1-2.83 0L2 12V2h10l8.59 8.59a2 2 0 0 1 0 2.82z"/><line x1="7" y1="7" x2="7.01" y2="7"/></svg>
                            {%- for tag in article_tags -%} 
                                {%- if tag contains 'blog_' -%}{%- continue -%}{%- endif -%}
                                <a href="{{ blog_url }}/tagged/{{ tag | handle }}">{{ tag }}</a> {%- unless forloop.last -%} ,  {% endunless -%}
                            {%- endfor -%} 
                        </div>
                    {%- endif -%}
                    {%- if se_stts.show_author or se_stts.show_comment -%}
                        <div class="t4s-article-metas">
                            {%- if se_stts.show_author -%}
                                <div class="t4s-article-author">
                                    <span class="t4s-article-author__icon"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"/><circle cx="12" cy="7" r="4"/></svg></span><span class="t4s-article-author__name">{{ 'blogs.article.by' | t }} : {{ article.author | replace_first: 'ad clnt', 'admin' }}</span>
                                </div>
                            {%- endif -%}
                            {%- if se_stts.show_comment -%}
                                {%- if article.comments_count > 0 %}{% assign cm_link = '#comments' %}{% else %}{% assign cm_link = '#CommentForm' %}{% endif -%}
                                <div class="t4s-article-comment">
                                    <a href="{{ article.url }}{{ cm_link }}" class="t4s-d-inline-block">
                                        <span class="t4s-article-comment__icon"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-message-square"><path d="M21 15a2 2 0 0 1-2 2H7l-4 4V5a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2z"/></svg></span><span class="t4s-article-comment__count">{{ 'blogs.comments.with_count_html' | t: count: article.comments_count }}</span>
                                    </a>
                                </div>
                            {%- endif -%}
                        </div>	
                    {%- endif -%}
                    {%- if se_stts.show_date -%}
                        {%- assign date = se_stts.date -%}
                        <div class="t4s-article-date">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-calendar"><rect x="3" y="4" width="18" height="18" rx="2" ry="2"/><line x1="16" y1="2" x2="16" y2="6"/><line x1="8" y1="2" x2="8" y2="6"/><line x1="3" y1="10" x2="21" y2="10"/></svg>
                            {%- if date contains "%" -%}
                                {{ article.published_at | time_tag: date }}
                            {%- else -%}	
                                {{ article.published_at | time_tag: format: date }}
                            {%- endif -%}
                        </div>
				    {%- endif -%}
                </div>
            </div>
            <div class="t4s-col-item t4s-col-lg-9 t4s-col-md-9 t4s-col-12 t4s-main-article-page">
                {%- for block in section.blocks -%}
                    {%- assign bk_stts = block.settings -%}
                    {%- case block.type -%}
                        {%- when 'content' -%}
                            <div class="t4s-article-content t4s-article-content__{{ bk_stts.dessign_content }}">
                                <article id="t4s-article-{{ article_id }}" class="article-{{ article_id }}">{{ article.content }}</article>
                            </div>
                        {%- when 'tags' -%}
                            <div class="t4s-article-tags">
                                {%- if article_tags.size > 0 -%}                             
                                    <ul class="t4s-article-tags__list t4s-article-tags__count-{{ bk_stts.count }}">
                                        {%- for tag in article_tags -%} 
                                            {%- if tag contains 'blog_' -%}{%- continue -%}{%- endif -%}
                                            <li class="t4s-d-inline-block"><a href="{{ blog_url }}/tagged/{{ tag | handle }}">{{ tag }} <span class="t4s-tag-count">({{ tag.total_count }})</span></a></li>
                                        {%- endfor -%}
                                    </ul>
                                {%- endif -%}
                            </div>                        
                        {%- when '@app' -%}<div class="t4s-page-block--app" {{ block.shopify_attributes }}>{% render block %}</div>
                        {%- when 'socials' -%}
                            {% liquid 
                                assign pri_cl_lightness = bk_stts.pri_cl | color_extract: 'lightness'
                                assign pri_hover_cl_lightness = bk_stts.pri_hover_cl | color_extract: 'lightness'
                                if pri_cl_lightness < 85
                                    assign pri_cl2 = "#fff"
                                else 
                                    assign pri_cl2 = "#000"
                                endif
                                if pri_hover_cl_lightness < 85
                                    assign pri_hover_cl2 = "#fff"
                                else 
                                    assign pri_hover_cl2 = "#000"
                                endif
                            -%}
                            {{ 'icon-social.css' | asset_url | stylesheet_tag }}
                            <div class="t4s-article-socials" style="--pri-cl: {{ bk_stts.pri_cl }};--pri-cl2: {{ pri_cl2 }};--pri-hover-cl: {{ bk_stts.pri_hover_cl }};--pri-hover-cl2: {{ pri_hover_cl2 }};--mgb: {{ bk_stts.mgb }}px;--mgb-tb: {{ bk_stts.mgb_tb }}px;--mgb-mb: {{ bk_stts.mgb_mb }}px; --bd-radius: {{ bk_stts.bd_radius }}px;">
                                {%- if bk_stts.social_mode == '1' -%} 
                                    {%- assign follow_social = true -%} 
                                {%- else -%} 
                                    {%- assign share_image = settings.share_image | default: page_image | default: settings.logo -%} 
                                {%- endif -%} 
                                {%- render 'social_sharing', style: bk_stts.social_style, use_color_set: bk_stts.use_color_set, size: bk_stts.social_size, space_h_item: bk_stts.space_h_item, space_h_item_mb: bk_stts.space_h_item_mb, space_v_item: bk_stts.space_v_item, space_v_item_mb: bk_stts.space_v_item_mb, share_permalink: shop.url, share_title: shop.name, share_image: share_image, follow_social: follow_social -%} 
                            </div> 
                        {%- when 'navigation' -%}
                            <div class="t4s-article-navigation t4s-d-flex t4s-align-items-center t4s-justify-content-center">
                                {%- if blog.previous_article -%}
                                    <a href="{{ blog.previous_article.url }}" class="t4s-d-block" data-tooltip="top" title="{{ blog.previous_article.title }}">
                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="32" height="32" viewBox="0 0 32 32">
                                            <path d="M12.792 15.233l-0.754 0.754 6.035 6.035 0.754-0.754-5.281-5.281 5.256-5.256-0.754-0.754-3.013 3.013z"/>
                                        </svg>
                                    </a>
                                {%- else -%}
                                    <a href="{{ blog_url }}" class="t4s-d-block t4s-op-0">
                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="32" height="32" viewBox="0 0 32 32">
                                            <path d="M12.792 15.233l-0.754 0.754 6.035 6.035 0.754-0.754-5.281-5.281 5.256-5.256-0.754-0.754-3.013 3.013z"/>
                                        </svg>
                                    </a>
                                {% endif -%}
                                <a href="{{ blog_url }}" class="t4s-current-blog" class="t4s-d-block" data-tooltip="top" title="{{ 'blogs.article.back_to' | t: title: blog.title }}">
                                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="32" height="32" viewBox="0 0 32 32">
                                        <path d="M6.937 21.865c-1.766 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM6.937 27.195c-1.176 0-2.132-0.956-2.132-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                        <path d="M6.937 3.738c-1.766 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM6.937 9.069c-1.176 0-2.132-0.956-2.132-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                        <path d="M6.937 12.779c-1.766 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM6.937 18.11c-1.176 0-2.132-0.957-2.132-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                        <path d="M16 21.865c-1.767 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM16 27.195c-1.176 0-2.133-0.956-2.133-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                        <path d="M16 3.738c-1.767 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM16 9.069c-1.176 0-2.133-0.956-2.133-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                        <path d="M16 12.779c-1.767 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM16 18.11c-1.176 0-2.133-0.957-2.133-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                        <path d="M25.063 21.865c-1.767 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM25.063 27.195c-1.176 0-2.133-0.956-2.133-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                        <path d="M25.063 10.135c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198c-1.767 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199zM25.063 4.805c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133c-1.176 0-2.133-0.956-2.133-2.133s0.956-2.132 2.133-2.132z" fill="currentColor"/>
                                        <path d="M25.063 12.779c-1.767 0-3.199 1.432-3.199 3.198s1.432 3.199 3.199 3.199c1.766 0 3.199-1.432 3.199-3.199s-1.433-3.198-3.199-3.198zM25.063 18.11c-1.176 0-2.133-0.957-2.133-2.133s0.956-2.132 2.133-2.132c1.176 0 2.133 0.956 2.133 2.132s-0.956 2.133-2.133 2.133z" fill="currentColor"/>
                                    </svg>
                                </a>
                                {%- if blog.next_article -%}
                                    <a href="{{ blog.next_article.url }}" class="t4s-d-block" data-tooltip="top" title="{{ blog.next_article.title }}">
                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="32" height="32" viewBox="0 0 32 32">
                                            <path d="M19.159 16.767l0.754-0.754-6.035-6.035-0.754 0.754 5.281 5.281-5.256 5.256 0.754 0.754 3.013-3.013z"/>
                                        </svg>
                                    </a>
                                {%- else -%}
                                    <a href="{{ blog_url }}" class="t4s-d-block t4s-op-0">
                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" width="32" height="32" viewBox="0 0 32 32">
                                            <path d="M19.159 16.767l0.754-0.754-6.035-6.035-0.754 0.754 5.281 5.281-5.256 5.256 0.754 0.754 3.013-3.013z"/>
                                        </svg>
                                    </a>
                                {%- endif -%}
                            </div>
                        {%- when 'comments' -%}{% unless article.comments_enabled? %}{% continue %}{% endunless -%}
                            <div class="t4s-article-comments t4s-oh">
                                {%- if number_of_comments > 0 -%}
                                    {%- assign anchorId = '#Comments-' | append: article.id -%}
                                    {%- paginate article.comments by 5 -%}
                                        {%- comment -%}#comments is required, it is used as an anchor link by Shopify.{%- endcomment -%}
                                        <div id="comments">
                                            <h2 id="Comments-{{ article.id }}" class="t4s-comments-heading t4s-w-100 t4s-text-center">{{ 'blogs.comments.comments_title_html' | t: count: number_of_comments, title: article_title }}</h2>
                                            <div class="t4s-comment__list t4s-row t4s-gy-40">
                                                {%- if comment.status == 'pending' and comment.content -%}
                                                    {%- comment -%}If a comment was just submitted with no blank field, show it. {%- endcomment -%}
                                                    <div id="{{ comment.id }}" class="t4s-comment__item t4s-col-item t4s-col-12">{%- render 'comment', comment: comment -%}</div>
                                                {%- endif -%}
                                                {%- for comment in article.comments -%}
                                                    <div id="{{ comment.id }}" class="t4s-comment__item t4s-col-item t4s-col-12">{%- render 'comment', comment: comment -%}</div>
                                                {%- endfor -%}
                                                {%- if paginate.pages > 1 -%}
                                                    <div class="t4s-col-12">{%- render 'pagination', paginate: paginate, anchor: anchorId, ajax: 'false' -%}</div>
                                                {%- endif -%}
                                            </div>
                                        </div>
                                    {%- endpaginate -%}
                                {%- endif -%}
                                <div class="t4s-line-space"></div>
                                {%- form 'new_comment', article, id: 'CommentForm' -%}
                                    <h2 class="t4s-comments-form__heading t4s-text-center">{{ 'blogs.comments.title' | t }}</h2>
                                    <p class="t4s-comments-form__notes t4s-text-center">{{ 'blogs.comments.comments_note_html' | t }}</p>
                                    {%- liquid
                                        assign post_message  = 'blogs.comments.success'
