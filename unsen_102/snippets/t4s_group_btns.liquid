      <a data-link-compare class="t4s-pr" href="{{ routes.search_url }}/?view=compare">
      	{%- if se_stts.h_txt_lable -%} {{ 'customer.menu.compare' | t }} {%- else -%} <svg class="t4s-icon t4s-icon--compare" aria-hidden="true" focusable="false" role="presentation"><use href="#icon-h-compare"></use></svg> {%- endif -%}
		  <span data-count-compare class="t4s-pa t4s-op-0 t4s-ts-op t4s-count-box">0</span>
      </a>
   </div>
   {%- endif -%}
	{%- if se_stts.show_wis -%}
   <div class="t4s-site-nav__icon t4s-site-nav__heart t4s-d-none t4s-d-lg-inline-block">
      {%- if settings.wishlist_mode != '3' -%}	
	      <a data-link-wishlist class="t4s-pr" href="{{ routes.search_url }}/?view=wishlist">
	      	{%- if se_stts.h_txt_lable -%} {{ 'customer.menu.wishlist' | t }} {%- else -%} <svg class="t4s-icon t4s-icon--heart" aria-hidden="true" focusable="false" role="presentation"><use href="#icon-h-heart"></use></svg> {%- endif -%}
			  <span data-count-wishlist class="t4s-pa t4s-op-0 t4s-ts-op t4s-count-box">0</span>
