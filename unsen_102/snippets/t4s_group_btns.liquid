			.t4s-h-cart__design5 .t4s-site-nav__cart .t4s-count-box {
				display: none
			}
			.t4s-h-cart__design4 .t4s-h-cart-totals {
				margin-left: 7px;
			}
			.t4s-h-cart__design5 .t4s-h-cart__divider {
